$ORIGIN {{ coredns_domain }}.
@       3600 IN SOA ns.{{ coredns_domain }}. admin.{{ coredns_domain }}. (
				2017042745 ; serial
				7200       ; refresh (2 hours)
				3600       ; retry (1 hour)
				1209600    ; expire (2 weeks)
				3600       ; minimum (1 hour)
				)
        3600 IN NS ns.{{ coredns_domain }}.
{{ coredns_domain }}.                    IN  A   {{ ingress_ips | join("\n@                    IN  A   ") }}
*.{{ coredns_domain }}.                   IN  CNAME {{ coredns_domain }}.
